weightmedian <- function (directory, day {
files <- list.files(directory, full.names=TRUE)
data <- data.frame()
for(i,1:length(files)) {
dat <- rbind(dat, read.csv(files[i]))
}
data_subset <- data[which(dat[,"Day" == day),]
median(data_subset[,"Weight"], na.rm=TRUE)
}
